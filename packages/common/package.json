{
  "name": "@hzyhhh/common",
  "version": "1.2.3",
  "description": "",
  "main": "lib/index.modern.js",
  "module": "lib/index.module.ts",
  "scripts": {
    "prettier": "prettier --config .prettierrc --write \"packages/**/*.ts?(x)\"",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "build": "microbundle -f modern --name index --compress false src/index.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "function"
  ],
  "lint-staged": {
    "*.ts?(x)": [
      "npm run prettier",
      "git add"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.12.0-next.8"
  }
}
